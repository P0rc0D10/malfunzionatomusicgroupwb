{
    "name": "velak",
    "version": "0.0.0",
    "description": "https://velak.klingt.org",
    "private": true,
    "scripts": {
        "clean": "rm -rf public/ && rm -f static/style.css*",
        "dev:style": "sass style/main.scss static/style.css --watch",
        "dev:hugo": "hugo server -D",
        "start": "npm run dev:hugo",
        "build:style": "sass style/main.scss static/style.css",
        "build:hugo": "hugo",
        "build": "npm run build:style && npm run build:hugo",
        "prod:sass": "sass style/main.scss static/style.css --no-source-map --style compressed",
        "prod:hugo": "hugo --environment production --verbose",
        "prod": "npm run clean && npm run prod:sass && npm run prod:hugo"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/disktree/velak.git"
    },
    "author": "tong@disktree.net",
    "license": "ISC",
    "homepage": "https://github.com/disktree/velak/blob/master/README.md",
    "devDependencies": {
        "sass": "^1.53.0"
    }
}
