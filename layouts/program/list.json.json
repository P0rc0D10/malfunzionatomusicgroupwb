[
    {{ range .Pages }}
    {
        "id": "{{ .File.ContentBaseName }}",
        "date": "{{ .Date.Format "2006/01/02" }}",
        "location": "{{ .Params.location }}",
        {{ with .Params.photos }}"photos": "{{.}}",{{ end }}
        "artists": [
            {{ with .Params.artists }}{{ range . }}"{{ . }}", {{ end }}{{ end }}
        ],
        "collabs": [
            {{ with .Params.collabs }}{{ range . }}"{{ . }}", {{ end }}{{ end }}
        ],
        "tags": [
            {{ with .Params.tags }}{{ range . }}"{{ . }}", {{ end }}{{ end }}
        ],
    },
    {{ end }}
]
